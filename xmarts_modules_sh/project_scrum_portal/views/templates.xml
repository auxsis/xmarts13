         <template id="projects_portel" name="My Project"
                  inherit_id="project.portal_my_projects"
                  customize_show="True">
            <xpath expr="//t[@t-call='portal.portal_searchbar']" position="replace">
                <h3 class="page-header">
                    <ol class="breadcrumb mt8">
                        <li style="font-size: 15px;">
                            <a t-att-href="'/my/home'+project_val">
                                <i class="fa fa-home"/>
                            </a>
                        </li>
                        <li style="font-size: 15px;">My Projects</li>
                    </ol>
                </h3>
            </xpath>
        </template>
         <template id="tasks_portel" name="My Tasks"
                  inherit_id="project.portal_my_tasks" customize_show="True">
            <xpath expr="//t[@t-call='portal.portal_searchbar']" position="replace">
                <h3 class="page-header">
                    <ol class="breadcrumb mt8">
                        <li style="font-size: 15px;">
                            <a t-att-href="'/my/home'+project_val">
                                <i class="fa fa-home"/>
                            </a>
                        </li>
                        <li style="font-size: 15px;">My Tasks</li>
                    </ol>
                </h3>
            </xpath>
        </template> 

         <template id="project_portel" name="My Projects"
                  inherit_id="project.portal_my_project" customize_show="True">
            <xpath expr="//t[@t-call='portal.portal_layout']"
                   position="after">
                <div class="oe_structure">
                    <div class="row">
                        <div class="col-sm-4">
                            <ol class="breadcrumb mt8">
                                <li>
                                    <a href="/my/home">
                                        <i class="fa fa-home"/>
                                    </a>
                                </li>
                                <li>
                                    <a t-attf-href="/my/projects?#{keep_query()}">
                                        My Projects
                                    </a>
                                </li>
                                <li>
                                    <span t-field="project.name"/>
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </xpath>
        </template> 

         <template id="task_portel" name="My Task"
                  inherit_id="project.portal_my_task" customize_show="True">
            <xpath expr="//t[@t-call='portal.portal_layout']"
                   position="after">
                <div class="oe_structure">
                    <div class="row">
                        <div class="col-sm-6">
                            <ol class="breadcrumb mt8">
                                <li>
                                    <a href="/my/home">
                                        <i class="fa fa-home"/>
                                    </a>
                                </li>
                                <li>
                                    <a t-attf-href="/my/tasks?#{keep_query()}">
                                        My Tasks
                                    </a>
                                </li>
                                <li>
                                    <span t-field="task.name"/>
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </xpath>
        </template> 

