<odoo>
    <data>

         <template id="sale.portal_my_home_sale"
                  name="Portal My Home: project entries"
                  inherit_id="portal.portal_my_home" priority="20">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <h3 class="page-header">
                    <span class="fa fa-quote-left mr10"/>
                     <a t-att-href="'/my/quotes'+project_val">Quotations</a>
                </h3>
                <h3 class="page-header">
                    <span class="fa fa-first-order mr10"/>
                    <a t-att-href="'/my/orders'+project_val">Sales Orders</a>
                </h3>
            </xpath>
        </template>

        <template id="account.portal_my_home_invoice"
                  name="Portal My Home : invoice entries"
                  inherit_id="portal.portal_my_home" priority="30">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <h3 class="page-header">
                    <span class="fa fa-usd mr10"/>
                    <a t-att-href="'/my/invoices'+project_val">Invoices</a>
                </h3>
            </xpath>
        </template> 

         <template id="portal.portal_breadcrumbs" name="Portal Breadcrumbs">
	        <ol t-if="page_name != 'home'" class="o_portal_submenu breadcrumb mb-0 py-2 flex-grow-1">
	            <li class="breadcrumb-item">
	                <t t-set="projects"
                        t-value="request.env['project.project'].sudo().search([('privacy_visibility', '=', 'portal')])"/>
                    <t t-if="projects">
                       <a t-attf-href="/my/home?{{ keep_query(project=projects[0].id) }}" aria-label="Home" title="Home">
                        <i class="fa fa-home"/></a>
                    </t>
	             </li>
	            <li t-if="page_name == 'my_details'" class="breadcrumb-item">Your Details</li>
	        </ol>
        </template>

         <template id="project.portal_my_home"
                  name="Portal My Home: project entries"
                  inherit_id="portal.portal_my_home" priority="15">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <h3 style="background-color: #3e3e3e;">
                    <div class="dropdown select_project">
                        <input type="hidden" class="project" name="project"
                               value=""/>
                        <t t-if="not project">
                            <button id="project_filters" class="" type="button"
                                    data-toggle="dropdown"
                                    style="background: transparent; border: none; padding-left: 0; padding-right: 0; color: #63cdf4;">
                                <span class="fa fa-check-circle-o mr10"/>
                                <span t-esc="project_filters.get(project, {}).get('label', projects[0].name)"/>
                                <span class="caret"></span>
                            </button>
                        </t>
                        <t t-if="project">
                            <t t-set="project"
                               t-value="request.env['project.project'].sudo().search([('id', '=', project)])"/>
                            <button id="project_filters" class="" type="button"
                                    data-toggle="dropdown"
                                    style="background: transparent; border: none; padding-left: 0; padding-right: 0; color: #63cdf4;">
                                <span class="fa fa-check-circle-o mr10"/>
                                <span t-esc="project_filters.get(project, {}).get('label', project.name)"/>
                                <span class="caret"></span>
                            </button>
                        </t>
                        <t t-if="not projects">
                            <div class="alert alert-info mt16" align="center">
                                <span>No Project Found.</span>
                            </div>
                        </t>
                        <ul class="dropdown-menu"
                            aria-labelledby="project_filters">
                            <t t-foreach="project_filters" t-as="option">
                                <li t-att-class="project == 'option' and 'active' or 'project'"
                                    t-att-value="option">
                                    <a t-attf-href="/my/home?{{ keep_query('date_begin', 'date_end', 'sortby', project=option) }}">
                                        <span t-esc="project_filters.get(option).get('label')"/>
                                    </a>
                                </li>
                            </t>
                        </ul>
                    </div>
                </h3>
                <h3 class="page-header">
                    <span class="fa fa-pie-chart mr10"/>
                    <a t-att-href="'/my/projects'+project_val">Projects
                    </a>
                </h3>
                <h3 class="page-header">
                    <span class="fa fa-clipboard mr10"/>
                    <a t-att-href="'/my/tasks'+project_val">Tasks
                    </a>
                </h3>
            </xpath>
        </template>


        <template id="portal_my_home" name="Portal My Home: Sprint entries"
                  inherit_id="portal.portal_my_home" priority="18">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <h3 class="page-header">
                    <span class="fa fa-th-list mr10"/>
                    <a t-att-href="'/my/sprints'+project_val">Sprint Planning
                    </a>
                </h3>
                <h3 class="page-header">
                    <span class="fa fa-indent mr10"/>
                    <a t-att-href="'/my/backlogs'+project_val">Product
                        Backlog
                    </a>
                </h3>
                <h3 class="page-header">
                    <span class="fa fa-calendar mr10"/>
                    <a t-att-href="'/my/meetings'+project_val">Your Meeting</a>
                </h3>
            </xpath>
        </template>
        
            <template id="portal.my_account_link" name="Link to frontend portal" inherit_id="portal.frontend_layout">
		        <xpath expr="//a[@id='o_logout']" position="before">
		            <t t-set="projects"
                        t-value="request.env['project.project'].sudo().search([('privacy_visibility', '=', 'portal')])"/>
		            <t t-if="projects">
		              <a t-attf-href="/my/home?{{ keep_query(project=projects[0].id) }}"
                           role="menuitem" class="dropdown-item">My Account
                        </a>
		            </t>
		        </xpath>
        </template>
        
        <template id="sidebar_form" name="Sidebar Form"
                  inherit_id="portal.portal_layout" customize_show="True">
            <xpath expr="//div[hasclass('o_portal_my_details')]" position="attributes">
                <attribute name="class">d-none oe_structure</attribute>
            </xpath>
            <xpath expr="//div[hasclass('justify-content-between')]" position="attributes">
                <attribute name="class">row justify-content-between</attribute>
            </xpath>
            <xpath expr="//div[hasclass('col-12','col-md','col-lg-4','col-xl-3','o_my_sidebar')]"
                   position="attributes">
                <attribute name="class">d-none col-12 col-md col-lg-4 col-xl-3 o_my_sidebar
                </attribute>
            </xpath>
            <xpath expr="//div[hasclass('container','mb64')]" position="attributes">
                <attribute name="class">page_dashboard container-fluid
                </attribute>
            </xpath>
             <xpath expr="//div[@t-attf-class='col-12 col-md col-lg-6']"
                   position="attributes">
                <attribute name="t-attf-class">col-12 col-md col-lg-12</attribute>
            </xpath>
            <xpath expr="//div[@class='o_portal container mt-3']"
                   position="attributes">
                <attribute name="class">container</attribute>
            </xpath>
        </template>
        <template id="my_home_page" name="My Home Page"
                  inherit_id="portal.portal_my_home" customize_show="True">
        <xpath expr="//div[hasclass('o_portal_my_home')]"
               position="attributes">
            <attribute name="class">o_portal_my_home sidebar-inner-wrapper
                col-md-3
            </attribute>
        </xpath>
         <xpath expr="//div[hasclass('o_portal_my_home')]/h3" position="replace">
            <h3 class="d-none">Your Documents</h3>
        </xpath> 
    </template>
        
